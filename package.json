{
  "name": "miniscript-ts",
  "version": "0.1.3",
  "description": "MiniScript implementation in TypeScript",
  "author": "Sebastian Nozzi",
  "license": "MIT",
  "keywords": ["miniscript", "language", "vm", "interpreter"],
  "repository": {
    "type": "git",
    "url": "https://github.com/sebnozzi/miniscript.ts"
  },
  "homepage": "https://github.com/sebnozzi/miniscript.ts",
  "main": "./dist/miniscript-ts.cjs",
  "module": "./dist/miniscript-ts.mjs",
  "exports": {
    ".": {
      "require": "./dist/miniscript-ts.cjs",
      "import": "./dist/miniscript-ts.mjs"
    }
  },
  "types": "./dist/miniscript-ts.d.ts",
  "files": [
    "dist"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "rimraf dist && tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "test": "vitest",
    "test-ui": "vitest --ui"
  },
  "devDependencies": {
    "@types/css-font-loading-module": "^0.0.12",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.4",
    "@vitest/ui": "^1.0.4",
    "dts-bundle-generator": "^9.0.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.0.4"
  }
}
